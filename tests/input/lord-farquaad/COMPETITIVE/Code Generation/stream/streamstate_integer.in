procedure main() returns integer {
  integer x;
  integer i;

  // input: 9999999999999999
  x <- std_input;
  x -> std_output;
  ' ' -> std_output;
  i = stream_state(std_input);
  i -> std_output;
  '\n' -> std_output;

  // input: 1
  x <- std_input;
  x -> std_output;
  ' ' -> std_output;
  i = stream_state(std_input);
  i -> std_output;
  '\n' -> std_output;

  // EOF
  x <- std_input;
  x -> std_output;
  ' ' -> std_output;
  i = stream_state(std_input);
  i -> std_output;
  '\n' -> std_output;

  return 0;
}