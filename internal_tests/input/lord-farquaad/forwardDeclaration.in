
function f(integer x) returns integer;
procedure pr(integer x) returns integer;

procedure main() returns integer {
   integer a = f(5);
   a -> std_output;
   '\n' -> std_output;
   call pr(10);
   '\n' -> std_output;
   return 0;
}

procedure pr(integer x) returns integer{
    x ->std_output;

    return 0;
}

function f(integer x) returns integer {
    return 2*x;
}

