procedure main() returns integer {

  [1,2,3,4,5,6,7,8][0] -> std_output;
  '\n' -> std_output;
  [1,2,3,4,5,6,7,8][4] -> std_output;
  '\n' -> std_output;
  [1,2,3,4,5,6,7,8][7] -> std_output;
  '\n' -> std_output;

  integer[10] a = 1..10;
  integer[11] b;
  integer c = 5;

  a -> std_output;
  '\n' -> std_output;

  a[8] = c;
  a -> std_output;
  '\n' -> std_output;

  c = 99;
  a->std_output;
  '\n' -> std_output;

  b = a;

  a->std_output;
  '\n' -> std_output;
  b->std_output;

  a[3] = 109;

  '\n' -> std_output;
  a->std_output;
  '\n' -> std_output;
  b->std_output;
  '\n' -> std_output;

  return 0;
}
