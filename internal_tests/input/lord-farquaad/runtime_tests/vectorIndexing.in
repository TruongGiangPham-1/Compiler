procedure main() returns integer {

  [1,2,3,4,5,6,7,8][1] -> std_output;
  '\n' -> std_output;
  [1,2,3,4,5,6,7,8][5] -> std_output;
  '\n' -> std_output;
  [1,2,3,4,5,6,7,8][8] -> std_output;
  '\n' -> std_output;

  integer[10] a = 1..10;
  integer[11] b;
  integer c = 5;

  a -> std_output;
  '\n' -> std_output;

  a[9] = c;
  a -> std_output;
  '\n' -> std_output;

  c = 99;
  a->std_output;
  '\n' -> std_output;

  b = a;

  a->std_output;
  '\n' -> std_output;
  b->std_output;

  a[4] = 109;

  '\n' -> std_output;
  a->std_output;
  '\n' -> std_output;
  b->std_output;
  '\n' -> std_output;

  return 0;
}
