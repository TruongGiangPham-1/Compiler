procedure main() returns integer {

  [1, 2, 3, 4] + [2, 2, 2, 2] -> std_output;
  '\n' -> std_output;
  [1, 2, 3, 4] + 2.3 -> std_output;
  '\n' -> std_output;
  [1, 2, 3] == [1, 2, 3] -> std_output;
  '\n' -> std_output;
  [1, 1, 3] == [1, 2, 3] -> std_output;
  '\n' -> std_output;

  var a = [1,2,3] ** [1,2,3];
  var b = [1,2,3] ** [4,5,6];
  a -> std_output;
  '\n' -> std_output;
  b -> std_output;
  '\n' -> std_output;

  return 0;
}
