procedure predicatedTest() returns integer {
    integer x = 0;
    loop while (x < 10) {
        return 10;
    }

    return 0;
}

procedure postPredicatedTest() returns integer {
    integer x = 0;
    loop {
        x = x + 1;
        if (x == 5) return x;
        else continue;
    } while (x < 10);

    return 0;

}

procedure main() returns integer {
    var x = predicatedTest();
    x -> std_output;
    x = postPredicatedTest();
    x -> std_output;

    return 0;
}