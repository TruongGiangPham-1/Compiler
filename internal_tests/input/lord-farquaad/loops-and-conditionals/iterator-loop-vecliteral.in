procedure main() returns integer {
  loop i in [1, 2, 3, 4, 5] {
    i -> std_output;
    ' ' -> std_output;
  }
  '\n' -> std_output;
  loop i in ['a', 'b', 'c', 'd', 'e'] {
    i -> std_output;
    ' ' -> std_output;
  }
  '\n' -> std_output;

  loop i in "hello world" {
      i -> std_output;
      ' ' -> std_output;
    }
    '\n' -> std_output;

  loop i in reverse("hello world") {
      i -> std_output;
      ' ' -> std_output;
    }
    '\n' -> std_output;

  return 0;
}
